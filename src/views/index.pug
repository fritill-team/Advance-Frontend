extends ./layouts/site.pug
include ./components/widgets/display-cards
include ./components/widgets/sliders
include ./components/banners
include ./components/service-excerpt

block title
  title My Page - Home

block content
  section.home-section
    .container
      +home-main-slider
  section.home-section
    .mask.mask--partners
    .container
      .partners
        .partners__content
          h2.partners__title Our Partners
