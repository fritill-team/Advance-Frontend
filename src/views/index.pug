            +blog-card("title course", "Lorem ipsum dolor sit amet, consectetur adipisicing elit. Assumenda blanditiis,")
            +blog-card("title course", "Lorem ipsum dolor sit amet, consectetur adipisicing elit. Assumenda blanditiis,")
