            img(src="../../assets/images/Wnbe.svg" alt="" class="partner__image-inverted")
          .partner
            p.partner__role Sponsored By
            img(src="../../assets/images/DrososLogoPrint.svg" alt="" class="partner__image")
            img(src="../../assets/images/Drosos2.svg" alt="" class="partner__image-inverted")

  section.home-section
    .container
      .row
        .col-sm-12.col-md-4.mb-3
